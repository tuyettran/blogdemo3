$('.table-body').html('<% @posts.each do |post| %>\
  <%= j render("post", post: post) %><% end %>');
$('.paginate').html('<%= j render("paginate", posts: @posts) %>');
$('.csv-link').html('<%= link_to "CSV Download", \
  posts_manager_search_path(:format => :csv, :post_key =>@post_key,\
  :user_key =>@user_key)%>')
